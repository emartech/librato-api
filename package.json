{
  "name": "librato-api",
  "version": "0.3.0",
  "description": "A Librato backend API client library and a simple CLI tool.",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm test && npm run eslint && npm run coverage && npm run coverage-check",
    "test": "NODE_ENV=test TZ=utc mocha --require co-mocha *.spec.js",
    "test-watch": "npm run test -- --watch",
    "eslint": "eslint .",
    "coverage": "nyc npm test >/dev/null && nyc report -r html -r text",
    "coverage-check": "nyc check-coverage --lines 100 --functions 100 --branches 100",
    "coverage-open": "open coverage/index.html"
  },
  "bin": {
    "librato": "./librato.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emartech/librato-api.git"
  },
  "keywords": [
    "librato",
    "api",
    "cli"
  ],
  "author": "JÃ¼rgen Strobel <juergen.strobel@emarsys.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emartech/librato-api/issues"
  },
  "homepage": "https://www.npmjs.com/package/librato-api",
  "dependencies": {
    "co": "^4.6.0",
    "js-combinatorics": "^0.5.2",
    "lodash": "^4.16.1",
    "mz": "^2.4.0",
    "request": "^2.75.0",
    "request-promise": "^4.1.1",
    "require-dir": "^0.3.0",
    "uuid": "^2.0.3",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "co-mocha": "^1.1.3",
    "eslint": "^3.5.0",
    "eslint-plugin-standard": "^2.0.0",
    "mocha": "^3.0.2",
    "nyc": "^8.3.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "node_modules/*",
      "example-config"
    ]
  }
}
